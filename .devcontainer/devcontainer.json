{
  "build": {
    "target": "frida-cshell"
  },
  "name": "Frida C-Shell",
  "dockerFile": "Dockerfile",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "rvest.vs-code-prettier-eslint",
        "ms-vscode.cpptools"
      ]
    }
  },
  "runArgs": [
    "--cap-add=SYS_PTRACE",
    "--security-opt",
    "seccomp=unconfined"
  ]
}
